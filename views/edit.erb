
<h2>Edit Bookmark</h2>

<form action="/bookmarks/<%= @bookmark.id %>" method="post">

<label for="name">Name: </label>
<input type="text" name="name" value="<%= @bookmark.name %>"></input>

<label for="address">Bookmark URL: </label>
<input type="text" name="address" value="<%= @bookmark.address %>"></input>

<input type="submit" value="Edit Bookmark"></input>

</form>

